{
    "name": "marcozp/cash",
    "type": "phpbb-extension",
    "description": "Cash Points System for phpBB 3.3.15",
    "homepage": "",
    "version": "1.0.0",
    "time": "2025-04-10",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "marcozp",
            "email": "",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0",
        "phpbb/phpbb": ">=3.3.0,<3.4.0@dev"
    },
    "autoload": {
        "psr-4": {
            "marcozp\\\\cash\\\\": "."
        }
    },
    "extra": {
        "display-name": "ZP Cash System",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<3.4.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/cash_points_system",
            "filename": "version_check",
            "ssl": true
        }
    }
}
